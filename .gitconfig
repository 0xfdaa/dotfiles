[user]
	name = k
	email = wer@w.pl
[color]
	ui = true
[alias]
	st = status --short
	lg = log --oneline --decorate --graph
	lgl = log --oneline --decorate --graph --stat
	cm = commit -m 
	difl = diff HEAD^
	d = diff
	ad = add .
	pom = push origin master
	
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
